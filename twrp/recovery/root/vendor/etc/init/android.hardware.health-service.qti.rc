service vendor.health-default /system/bin/android.hardware.health-service.qti
    class hal
    user root
    group root
    capabilities WAKE_ALARM BLOCK_SUSPEND
    file /dev/kmsg w
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib:/system/lib64:/system/lib:/sbin 
    disabled
    seclabel u:r:recovery:s0

service vendor.charger /system/bin/android.hardware.health-service.qti --charger
    class charger
    seclabel u:r:charger_vendor:s0
    user root
    group root wakelock input graphics
    capabilities SYS_BOOT WAKE_ALARM BLOCK_SUSPEND
    file /dev/kmsg w
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib:/system/lib64:/system/lib:/sbin 
    disabled
    seclabel u:r:recovery:s0
